                    Label:
                        text: 'Aspirante Selecionado:'
                        text_size: self.size
                        halign: 'left'
                        valign: 'middle'
                        color: 0.18, 0.28, 0.40, 1
                        bold: True 
                    Label:
                        text: app.numero_atual + ' | ' + app.nome_guerra
                        text_size: self.size
                        halign: 'left'
                        valign: 'middle'
                        color: 0.18, 0.28, 0.40, 1
                        bold: True
                    Label:
                        size_hint: 1,.3
                
                Informacoes_Licenca:
                    size_hint: 1, .1
                Dados_Gerais_Licenca:
                    size_hint: 1, .65

            Label:
                size_hint: .1, 1

        Rodape_Licenca:
            size_hint: 1, .05
    AnchorLayout:
        size_hint: .05, .05
        anchor_x: 'left'
        achor_y: 'bottom'
        Button:
            text: 'Menu'
            on_press:
                root.manager.transition.direction = 'right'
                root.manager.current = 'menu'
            background_color: 0.7, 0.56, 0.4, 1
            text_color: 0.77,0.77,0.77,1
            bold: True

