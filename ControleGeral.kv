#LICENCAS

<LicencaScreen>:

    chave_pesquisa: chave_pesquisa

    BoxLayout:
        canvas.before:
            Color:
                rgba: 0.77,0.77,0.77,1
            Rectangle:
                pos: self.pos
                size: self.size
        orientation: "vertical"
        Topo_Licenca:
            size_hint: 1, .1

        #Corpo Licenca
        BoxLayout:
            size_hint: 1, .85
            Label:
                size_hint: .1, 1

            #Interface licenca
            BoxLayout:
                size_hint: .8, 1
                orientation: "vertical"

                #Atualiza licenca
                BoxLayout:
                    size_hint: 1, .1
                    Label:
                        size_hint: .125, 1
                    Label:
                        size_hint: .1, 1
                        text: 'Novo status:'
                        color: 0.18, 0.28, 0.40, 1
                        bold: True
                    GridLayout:
                        size_hint: .65, 1
                        spacing: "5dp"
                        rows: 2
                        ToggleButton:
                            group: 'licenca'
                            id: reg_btn
                            text: 'Regresso'
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            id: lic_btn
                            text: 'Licença'
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            id: st_btn
                            text: 'ST/GT'
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            text: 'Baixado'
                            id: baixado_btn
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            text: 'Disp. Domiciliar'
                            id: dispdom_btn
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            text: 'LTS'
                            id: lts_btn
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            text: 'HNMD'
                            id: hnmd_btn
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                        ToggleButton:
                            group: 'licenca'
                            text: 'C/ Restrição'
                            id: restr_btn
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            background_color: 0.7, 0.56, 0.4, 1
                            text_color: 0.77,0.77,0.77,1
                            bold: True
                            on_press:
                                app.BOTAO_PRESSIONADO = self.text
                            on_release:
                                app.BOTAO_PRESSIONADO = ''
                    Label:
                        size_hint: .125, 1

                #Consulta licenca
                BoxLayout:
                    size_hint: 1, .1
                    spacing: "20dp"

                    Label:
                        size_hint: 1,.2

                    Label:
                        text: 'Número Interno/Nome de Guerra:'
                        size_hint: 1, 1
                        text_size: self.size
                        halign: 'right'
                        valign: 'middle'
                        color: 0.18, 0.28, 0.40, 1
                        bold: True

                    TextInput:
                        id: chave_pesquisa
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint: 1, .6
                        multiline: False
                        text_validate_unfocus: False
                        on_text_validate:
                            app.consultar_licenca(self.text)
                            app.atualiza_licenca(app.BOTAO_PRESSIONADO)
                            chave_pesquisa.text = ''

                    Button:
                        text: 'Selecionar'
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint: 1, .6
                        on_press:
                            app.consultar_licenca(chave_pesquisa.text)
                            app.atualiza_licenca(app.BOTAO_PRESSIONADO)
                            chave_pesquisa.text = ''
