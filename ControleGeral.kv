MenuScreen

#MENU

<MenuScreen>:
    BoxLayout:
        canvas.before:
            Color:
                rgba: 0.77,0.77,0.77,1
            Rectangle:
                pos: self.pos
                size: self.size
        orientation: "vertical"
        BoxLayout:
            size_hint: 1, .1
            Image:
                source: 'LogoM.png'
                size: self.texture_size
                size_hint: .57, 2
                pos_hint: {'center_x': 30, 'center_y': -0.2}
        
            Label:
                text: 'CONTROLE GERAL'
                color: 0.18, 0.28, 0.40, 1
                bold: True
                font_size: "28sp"
                size_hint: .8, 1

            Image:
                source: 'LogoEN.png'
                size: self.texture_size
                size_hint: .57, 2
                pos_hint: {'center_x': -30, 'center_y': -0.2}

        BoxLayout:
            size_hint: 1, .85
            Label:
                size_hint: .1, 1
            BoxLayout:
                size_hint: .8, 1
                spacing: "20dp"
                orientation: 'vertical'
                Button:
                    text: 'PBEN'
                    on_press: root.manager.current = 'pben'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    size_hint: .6,.1
                    bold: True
                Button:
                    text: 'Licença'
                    on_press: root.manager.current = 'licenca'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    size_hint: .6,.1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    bold: True
                Button
                    text: 'Claviculário'
                    on_press: root.manager.current = 'chaves'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    size_hint: .6,.1
                    bold: True
                Button
                    text: 'Acesso à Parte Baixa'
                    on_press: root.manager.current = 'partealta'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    size_hint: .6,.1
                    bold: True
                Button
                    text: 'Registro Chefe de Dia'
                    on_press: root.manager.current = 'chefedia'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    size_hint: .6,.1
                    bold: True
                Button:
                    text: 'SAIR'
                    background_color: 0.7, 0.56, 0.4, 1
                    text_color: 0.77,0.77,0.77,1
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    size_hint: .6,.1
                    on_press: app.stop() 
                    bold: True
            Label:
                size_hint: .1, 1
        BoxLayout:
            size_hint: 1, .05
            BoxLayout:
                size_hint: .1, 1
                Label:
                    size_hint: .1, 1
                Label:
                    size_hint: .2, 1
            Label:
                size_hint: .7, 1
            Label:
                text: 'Pelo Aspirante Hartmann'
                color: 0.18, 0.28, 0.40, 1
                bold: True
                size_hint: .2, 1

#PBEN

<PbenScreen>:
    chave_pesquisa: chave_pesquisa


    BoxLayout:
        canvas.before:
            Color:
                rgba: 0.77,0.77,0.77,1
            Rectangle:
                pos: self.pos
                size: self.size

        orientation: "vertical"

        #Topo
        BoxLayout:
            size_hint: 1, .1
            Image:
                source: 'LogoM.png'
                size: self.texture_size
                size_hint: .57, 2
                pos_hint: {'center_x': 30, 'center_y': -0.2}
        
            Label:
                text: 'PBEN'
                color: 0.18, 0.28, 0.40, 1
                bold: True
                font_size: "28sp"
                size_hint: .8, 1

            Image:
                source: 'LogoEN.png'
                size: self.texture_size
                size_hint: .57, 2
                pos_hint: {'center_x': -30, 'center_y': -0.2}

        #Corpo
        BoxLayout:
            size_hint: 1, .85
            Label:
                size_hint: .1, 1

            #Interface
            BoxLayout:
                size_hint: .8, 1
                orientation: "vertical"

                #Consulta
                BoxLayout:
                    size_hint: 1, .15
                    spacing: "20dp"

                    Label:
                        size_hint: 1,.2
                    Label:
                        text: 'Número Interno/Nome de Guerra:'
                        size_hint: 1, 1
                        text_size: self.size
                        halign: 'right'
                        valign: 'middle'
                        color: 0.18, 0.28, 0.40, 1
                        bold: True
                    TextInput:
                        id: chave_pesquisa
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint: 1, .4
                        multiline: False
                        text_validate_unfocus: False
                        on_text_validate:
                            app.consulta_pben(self.text)
                            self.text = ''
                    Button:
                        text: 'Consultar'
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint: 1, .4
                        on_press:
                            app.consulta_pben(chave_pesquisa.text)
                            chave_pesquisa.text = ''
                            chave_pesquisa.focus: True
                        background_color: 0.7, 0.56, 0.4, 1
                        text_color: 0.77,0.77,0.77,1
                        bold: True
                    Label:
                        size_hint: 1,.3

                DadosPrincipais:
                    size_hint: 1, .15

                OutrosDados:
                    text: 'outros dados'
                    size_hint: 1, .45

                Contatos:
                    size_hint: 1, .25

            Label:
                size_hint: .1, 1

        #Rodape
        BoxLayout:
            size_hint: 1, .05
            BoxLayout:
                size_hint: .1, 1
                Label:
                    size_hint: .1, 1
                Label:
                    size_hint: .2, 1
            Label:
                size_hint: .7, 1
            Label:
                text: 'Pelo Aspirante Hartmann'
                color: 0.18, 0.28, 0.40, 1
                bold: True
                size_hint: .2, 1

    AnchorLayout:
        size_hint: .05, .05
        anchor_x: 'left'
        achor_y: 'bottom'
        Button:
            text: 'Menu'
            on_press:
                root.manager.transition.direction = 'right'
                root.manager.current = 'menu'
            background_color: 0.7, 0.56, 0.4, 1
            text_color: 0.77,0.77,0.77,1
            bold: True


